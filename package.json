{
  "name": "@theprotagen/pwbooker",
  "version": "0.1.0-alpha.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "commit": "git-cz",
    "dev": "vite",
    "format": "prettier . --write",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "prepare": "husky install",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint --fix",
      "prettier --write"
    ],
    "package.json": "npx sort-package-json"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
